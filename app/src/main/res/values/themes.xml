<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<resources>

    <style name="BottomSheetDialog" parent="Theme.Design.Light.BottomSheetDialog">
        <item name="android:windowIsFloating">false</item>
        <item name="android:windowSoftInputMode">adjustResize|stateVisible</item>
    </style>

    <style name="ButtonDivider">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">?android:attr/listDivider</item>
    </style>

    <style name="Divider">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">?android:attr/listDivider</item>
    </style>

    <style name="FysButtonQrScan" parent="Widget.MaterialComponents.Button">
        <item name="backgroundTint">@null</item>
        <item name="android:foreground">?android:attr/selectableItemBackground</item>
        <item name="android:background">@drawable/gradient_qr</item>
        <item name="android:layout_width">60dp</item>
        <item name="android:layout_height">60dp</item>
        <item name="android:letterSpacing">0</item>
        <item name="icon">@drawable/ic_qr_code</item>
        <item name="iconGravity">textStart</item>
        <item name="iconPadding">0dp</item>
        <item name="iconSize">48dp</item>
        <item name="iconTint">@color/white</item>
    </style>

    <style name="FysDarkLargeText" parent="Widget.MaterialComponents.TextView">
        <item name="android:textColor">@color/text_normal</item>
        <item name="android:textSize">22sp</item>
        <item name="android:fontFamily">@font/poppins</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="FysDarkMediumText" parent="Widget.MaterialComponents.TextView">
        <item name="android:textColor">@color/text_normal</item>
        <item name="android:textSize">16sp</item>
        <item name="android:fontFamily">@font/poppins</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="FysDarkSmallText" parent="Widget.MaterialComponents.TextView">
        <item name="android:textColor">@color/text_normal</item>
        <item name="android:fontFamily">@font/poppins</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="FysPrimaryTextButton" parent="Widget.MaterialComponents.Button">
        <item name="backgroundTint">@null</item>
        <item name="android:stateListAnimator">@null</item>
        <item name="android:letterSpacing">0</item>
        <item name="android:background">@drawable/btn_ripple</item>
        <item name="android:fontFamily">@font/poppins</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="FysDarkTextButton" parent="Widget.MaterialComponents.Button">
        <item name="backgroundTint">@null</item>
        <item name="android:stateListAnimator">@null</item>
        <item name="android:letterSpacing">0</item>
        <item name="android:background">@drawable/btn_ripple</item>
        <item name="android:fontFamily">@font/poppins</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/text_normal</item>
    </style>

    <style name="FysFabQrScan" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">12dp</item>
    </style>

    <style name="FysIconButton" parent="Widget.MaterialComponents.Button">
        <item name="android:layout_height">33dp</item>
        <item name="android:layout_width">33dp</item>
        <item name="android:background">@drawable/btn_fys_gradient_corners</item>
        <item name="backgroundTint">@null</item>
        <item name="iconGravity">textStart</item>
        <item name="iconPadding">0dp</item>
        <item name="iconTint">@color/white</item>
    </style>

    <style name="FysWhiteIconButton" parent="Widget.MaterialComponents.Button">
        <item name="android:layout_height">33dp</item>
        <item name="android:layout_width">33dp</item>
        <item name="android:background">@drawable/btn_fys_white_corners</item>
        <item name="backgroundTint">@null</item>
        <item name="iconGravity">textStart</item>
        <item name="iconPadding">0dp</item>
        <item name="iconTint">@color/fys_primary</item>
    </style>

    <style name="FysIconButtonPrimary">
        <item name="android:layout_width">33dp</item>
        <item name="android:layout_height">33dp</item>
        <item name="android:scaleType">fitXY</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:tint">@drawable/selector_primary_btn_on_pressed</item>
    </style>

    <style name="FysImageButton">
        <item name="android:layout_width">30dp</item>
        <item name="android:layout_height">30dp</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:scaleType">fitXY</item>
        <item name="android:tint">@drawable/selector_white_btn_on_pressed</item>
    </style>

    <style name="FysLogo">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">45dp</item>
        <item name="android:layout_marginTop">28dp</item>
        <item name="android:importantForAccessibility">no</item>
        <item name="android:src">@drawable/fys_logo</item>
        <item name="android:tint">@color/white</item>
    </style>

    <style name="FysNoPaddingButton" parent="Widget.MaterialComponents.Button">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:background">@drawable/btn_fys_gradient_corners</item>
        <item name="android:insetTop">0dp</item>
        <item name="backgroundTint">@null</item>
        <item name="android:fontFamily">@font/poppins</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:insetBottom">0dp</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:letterSpacing">0</item>
        <item name="android:minWidth">0dp</item>
        <item name="android:minHeight">0dp</item>
    </style>

    <style name="FysPrimaryButton" parent="Widget.MaterialComponents.Button">
        <item name="backgroundTint">@null</item>
        <item name="android:letterSpacing">0</item>
        <item name="android:background">@drawable/btn_fys_gradient_corners</item>
        <item name="android:fontFamily">@font/poppins</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="FysWhiteButton" parent="Widget.MaterialComponents.Button">
        <item name="backgroundTint">@null</item>
        <item name="android:letterSpacing">0</item>
        <item name="android:background">@drawable/btn_fys_white_corners</item>
        <item name="android:fontFamily">@font/poppins</item>
        <item name="android:textColor">@color/fys_primary</item>
        <item name="android:gravity">center_vertical|start</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="FysWhiteSecondaryButton" parent="Widget.MaterialComponents.Button">
        <item name="backgroundTint">@null</item>
        <item name="android:letterSpacing">0</item>
        <item name="android:background">@drawable/btn_fys_white_corners</item>
        <item name="android:fontFamily">@font/poppins</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="FysPrimaryFullWidthButton" parent="Widget.MaterialComponents.Button">
        <item name="backgroundTint">@null</item>
        <item name="android:letterSpacing">0</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">65dp</item>
        <item name="android:background">@drawable/btn_fys_gradient</item>
        <item name="iconTint">@color/white</item>
        <item name="iconGravity">textStart</item>
        <item name="android:textSize">20sp</item>
        <item name="android:fontFamily">@font/poppins</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textAllCaps">false</item>
    </style>

    <style name="FysPrimaryLargeText" parent="Widget.MaterialComponents.TextView">
        <item name="android:textColor">@color/fys_primary</item>
        <item name="android:textSize">22sp</item>
        <item name="android:fontFamily">@font/poppins</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="FysSecondaryLargeText" parent="Widget.MaterialComponents.TextView">
        <item name="android:textColor">@color/fys_secondary</item>
        <item name="android:textSize">22sp</item>
        <item name="android:fontFamily">@font/poppins</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="FysSecondaryMediumText" parent="Widget.MaterialComponents.TextView">
        <item name="android:textColor">@color/fys_secondary</item>
        <item name="android:textSize">16sp</item>
        <item name="android:fontFamily">@font/poppins</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="FysPrimaryMediumText" parent="Widget.MaterialComponents.TextView">
        <item name="android:textColor">@color/fys_primary</item>
        <item name="android:textSize">16sp</item>
        <item name="android:fontFamily">@font/poppins</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="FysPrimarySmallText" parent="Widget.MaterialComponents.TextView">
        <item name="android:textColor">@color/fys_primary</item>
        <item name="android:fontFamily">@font/poppins</item>
        <item name="android:gravity">center</item>
    </style>


    <style name="FysSearchEditText">
        <item name="android:colorControlNormal">@color/fys_primary</item>
        <item name="android:colorControlActivated">@color/fys_primary</item>
        <item name="android:colorControlHighlight">@color/fys_primary</item>
    </style>

    <style name="FysSecondaryButton" parent="Widget.MaterialComponents.Button">
        <item name="android:foreground">?attr/selectableItemBackground</item>
        <item name="backgroundTint">@null</item>
        <item name="android:letterSpacing">0</item>
        <item name="android:background">@drawable/fys_white_rounded_corners</item>
        <item name="android:fontFamily">@font/poppins</item>
        <item name="android:textColor">@color/fys_primary</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:textSize">18sp</item>
    </style>

    <style name="FysSettingsButton" parent="Widget.MaterialComponents.Button">
        <item name="android:foreground">?attr/selectableItemBackground</item>
        <item name="backgroundTint">@null</item>
        <item name="android:letterSpacing">0</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:fontFamily">@font/poppins</item>
        <item name="android:gravity">left|center_vertical</item>
        <item name="android:textColor">@color/text_normal</item>
        <item name="android:layout_marginStart">16dp</item>
        <item name="android:layout_marginEnd">16dp</item>
        <item name="iconTint">@color/text_normal</item>
        <item name="android:padding">0dp</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="FysToolbar" parent="Widget.MaterialComponents.Toolbar">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">80dp</item>
        <item name="android:background">@drawable/fys_toolbar</item>
        <item name="android:contentInsetStart">0dp</item>
        <item name="android:minHeight">?attr/actionBarSize</item>
        <item name="android:theme">?attr/actionBarTheme</item>
    </style>

    <style name="FysWhiteChip">
        <item name="android:backgroundTint">@color/white</item>
        <item name="android:letterSpacing">0</item>
        <item name="android:textAlignment">center</item>
        <item name="android:fontFamily">@font/poppins</item>
        <item name="android:layout_height">55dp</item>
        <item name="chipMinHeight">50dp</item>
        <item name="chipMinTouchTargetSize">0dp</item>
        <item name="chipCornerRadius">10dp</item>
        <item name="android:textColor">@color/fys_secondary</item>
        <item name="android:textSize">18sp</item>
    </style>

    <style name="FysWhiteLargeText" parent="Widget.MaterialComponents.TextView">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">22sp</item>
        <item name="android:fontFamily">@font/poppins</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="FysWhiteMediumText" parent="Widget.MaterialComponents.TextView">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">16sp</item>
        <item name="android:fontFamily">@font/poppins</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="FysWhiteSmallText" parent="Widget.MaterialComponents.TextView">
        <item name="android:textColor">@color/white</item>
        <item name="android:fontFamily">@font/poppins</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="ShapeAppearance.App.SmallComponent" parent="ShapeAppearance.MaterialComponents.SmallComponent">
        <item name="cornerSize">2dp</item>
    </style>

    <style name="TabText" parent="Widget.MaterialComponents.TabLayout">
        <item name="android:fontFamily">@font/poppins</item>
        <item name="android:textSize">22sp</item>
    </style>

    <style name="Theme.FoodService" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <item name="colorPrimary">@color/fys_primary</item>
        <item name="colorOnPrimary">@color/white</item>
        <item name="colorSecondary">@color/fys_secondary</item>
        <item name="colorOnSecondary">@color/white</item>
        <item name="android:statusBarColor">@color/fys_primary</item>
        <item name="android:windowTranslucentStatus">true</item>
        <item name="android:windowDrawsSystemBarBackgrounds">true</item>
        <item name="android:windowTranslucentNavigation">true</item>
        <item name="android:ambientShadowAlpha">0.05</item>
        <item name="android:spotShadowAlpha">0.05</item>
    </style>

    <style name="Theme_Dialog" parent="android:Theme.Holo.Dialog">
        <item name="android:windowMinWidthMajor">90%</item>
        <item name="android:windowMinWidthMinor">90%</item>
    </style>

    <style name="bottomSheetStyleWrapper" parent="Widget.Design.BottomSheet.Modal">
        <item name="android:background">@android:color/transparent</item>
    </style>
</resources>
